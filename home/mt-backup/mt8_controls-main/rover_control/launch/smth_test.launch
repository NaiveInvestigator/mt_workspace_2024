<launch>
<arg name="rgb_port" default="/dev/ttyACM0"/>
<node name="status_led_node" pkg="rosserial_python" type="serial_node.py" args="$(arg rgb_port)" outputs="screen"/>
<arg name="key_topic" default="/rover_control"/>
<node name="talker_keyboard" pkg="rover_control" type="talker_keyboard.py" output="screen">
<param name="topic" value="$(arg key_topic)"/>
</node>   
</launch>

